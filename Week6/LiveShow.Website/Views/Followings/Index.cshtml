@model List<LiveShow.Services.Models.Followers.FollowerDto>
@{
    ViewData["Title"] = "Index";
}

<h1>Your Followers</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.First().FolloweeId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.First().FollowerId)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FolloweeId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FollowerId)
                </td>
            </tr>
        }
    </tbody>
</table>